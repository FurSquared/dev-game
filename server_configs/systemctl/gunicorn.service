[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=/var/www/backend
ExecStart=/var/www/backend/venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/var/www/backend/f2game.sock f2game.wsgi:application

[Install]
WantedBy=multi-user.target
